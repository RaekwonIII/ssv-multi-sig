{
    "compilerOptions": {
      "target": "es2022",
      "module": "nodenext",
      "lib": ["es2022"],
      "moduleResolution": "nodenext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "sourceMap": true,
    },
  }